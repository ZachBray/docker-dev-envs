{
    "vim.disableAnnoyingNeovimMessage": true,
    "vim.ignorecase": false,
    "vim.incsearch": false,
    "vim.easymotion": true,
    "vim.easymotionMarkerHeight": 16,
    "vim.easymotionMarkerFontWeight": "bold",
    "vim.easymotionMarkerBackgroundColor": "orange",
    "vim.easymotionMarkerForegroundColorOneChar": "darkblue",
    "vim.easymotionMarkerForegroundColorTwoChar": "darkblue",
    "vim.handleKeys": {
        "<C-j>": true,
        "<C-k>": true
    },
    "vim.insertModeKeyBindings": [{
        "before": ["<C-j>"],
        "after": ["<C-n>"],
        "commands": []
    }, {
        "before": ["<C-k>"],
        "after": ["<C-p>"],
        "commands": []
    }],
    "extensions.ignoreRecommendations": false,

    "checkstyle.autocheck": true,
    "checkstyle.version": "8.0",
    "checkstyle.configurationFile": "google_checks",
    "checkstyle.propertiesPath": "",

    "java.jdt.ls.vmargs": "-javaagent:/home/dev/lombok/lombok.jar -Xbootclasspath/a:/home/dev/lombok/lombok.jar"
}
